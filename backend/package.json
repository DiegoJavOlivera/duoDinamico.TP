{
  "name": "duodinamico",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./index.js",
    "dev": "nodemon --watch . ./index.js",
    "debug": "nodemon --inspect --watch . ./index.js",
    "db:sync": "node ./scripts/sync.js",
    "db:seed": "node ./scripts/seed.js",
    "db:drop": "node ./scripts/drop.js",
    "db:init": "npm run db:drop && npm run db:sync && npm run db:seed"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": ["express","sequelize","mysql","api","backend","nodejs","ejs","auth","multer","jwt"],
  "author": "Priscilla Escobar",
  "contributors": ["Diego Olivera", "Priscilla Escobar"],
  "license": "ISC",
  "description": "Backend de Duo Dinámico: API RESTful para gestión de productos, usuarios, autenticación y carrito de compras.",
  "techStack": [
    "Node.js",
    "Express.js",
    "Sequelize",
    "MySQL",
    "JWT (jsonwebtoken)",
    "Multer",
    "EJS",
    "dotenv",
    "bcrypt",
    "Nodemon"
  ],
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "nanoid": "^5.1.5",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
